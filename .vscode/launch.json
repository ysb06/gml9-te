            "env": {
                "NUMEXPR_MAX_THREADS": "10"
            },
            "justMyCode": false
        },
        {
            "name": "Run gm_lightning",
            "type": "debugpy",
            "request": "launch",
            "module": "gm_lightning",
            "console": "integratedTerminal",
            "env": {
                "NUMEXPR_MAX_THREADS": "10"
            },
            "justMyCode": false
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        }
    ]
